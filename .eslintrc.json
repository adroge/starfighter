{
	"plugins": [],
	"settings": {},
	"env": {
		"es6": true,
		"browser": true
	},
	"extends": [
		"eslint:recommended",
		"p5js",
		"p5js/dom",
		"p5js/sound"
	],
	"parserOptions": {
		"ecmaFeatures": {},
		"sourceType": "module"
	},
	"rules": {
		"no-console": 0,
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"linebreak-style": "off",
		"quotes": "off",
		"semi": "off",
		"no-undef": "warn",
		"no-unused-vars": "off"
	},
	"globals": {
		"config": true,
		"Bullets": true,
		"images": true,
		"sounds": true,
		"Enemy": true,
		"Ship": true,
		"Stars": true
	}
}